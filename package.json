{
  "name": "@aersosi/a11ytip",
  "version": "1.1.0",
  "description": "An accessible, customizable, css first tooltip system",
  "main": "src/ToggleA11ytip.ts",
  "style": "a11ytip.css",
  "types": "ToggleA11ytip.ts",
  "files": [
    "src/a11ytip.css",
    "src/a11ytip_config.css",
    "src/ToggleA11ytip.ts",
    "dist/ToggleA11ytip.min.js",
    "dist/a11ytip.min.css",
    "README.md",
    "README_de.md"
  ],
  "scripts": {
    "build": "tsc",
    "minify:js": "terser dist/ToggleA11ytip.js -o dist/ToggleA11ytip.min.js --compress --mangle && rimraf dist/ToggleA11ytip.js dist/ToggleA11ytip.d.ts",
    "minify:css": "lightningcss --minify --bundle --targets '>= 0.25%' src/a11ytip.css -o dist/a11ytip.min.css",
    "minify": "npm run minify:js && npm run minify:css",
    "build:prod": "npm run build && npm run minify"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "tooltip",
    "css",
    "typescript",
    "customizable",
    "aria",
    "dark-mode"
  ],
  "author": {
    "name": "Arthur Ersosi"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aersosi/a11ytip.git",
    "directory": "a11ytip"
  },
  "bugs": {
    "url": "https://github.com/aersosi/a11ytip/issues"
  },
  "homepage": "https://github.com/aersosi/a11ytip",
  "devDependencies": {
    "lightningcss-cli": "^1.30.1",
    "rimraf": "^6.0.1",
    "terser": "^5.19.2",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
